{
  "name": "console-patterns",
  "description": "Collection of snippets to render logical patterns",
  "version": "1.0.0",
  "repository": "git@github.com:grikomsn/console-patterns.git",
  "author": "Griko Nibras <grikomsn@gmail.com>",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "format": "prettier --config ./.prettierrc --write ./{patterns,src}/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn build:dir && yarn build:favicons && gatsby build",
    "create-index": "create-index ./{patterns,src/{components,helpers}} && yarn format",
    "now-build": "yarn build"
  },
  "dependencies": {
    "@sindresorhus/class-names": "^1.1.0",
    "copee": "^1.0.6",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "create-index": "^2.5.0",
    "favicons": "^6.0.0",
    "mkdirp": "^1.0.4",
    "node-gyp": "^6.1.0",
    "prettier": "^2.0.3"
  },
  "private": true,
  "license": "MIT"
}
