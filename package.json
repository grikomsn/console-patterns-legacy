{
  "name": "console-patterns",
  "description": "Collection of snippets to render logical patterns",
  "version": "1.0.0",
  "repository": "git@github.com:grikomsn/console-patterns.git",
  "author": "Griko Nibras <grikomsn@gmail.com>",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "format": "prettier --config ./.prettierrc --write ./{patterns,src}/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn build:dir && yarn build:favicons && gatsby build",
    "create-index": "create-index ./{patterns,src/{components,helpers}} && yarn format",
    "now-build": "yarn build"
  },
  "dependencies": {
    "@sindresorhus/class-names": "^1.1.0",
    "copee": "^1.0.6",
    "gatsby": "^2.13.61",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-offline": "^2.2.5",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-sitemap": "^2.2.6",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-json": "^2.2.2",
    "gatsby-transformer-sharp": "^2.2.6",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "create-index": "^2.4.0",
    "favicons": "^5.4.1",
    "mkdirp": "^0.5.1",
    "node-gyp": "^5.0.3",
    "prettier": "^1.18.2"
  },
  "private": true,
  "license": "MIT"
}
